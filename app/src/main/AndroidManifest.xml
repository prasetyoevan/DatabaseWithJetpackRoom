<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.vanevaan.kelas.databasewithroom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_jkt48_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_jkt48_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DatabaseWithRoom">
        <activity android:name="id.vanevaan.kelas.databasewithroom.activities.EditActivity"></activity>
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>